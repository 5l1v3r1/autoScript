
#!/bin/bash
CURRENT_DIR=$(pwd)
INPUT=${1:-run}
if [ $INPUT == run ]
then
cd $PERM_DIR
./autoScript.sh
elif [ $INPUT == update ]
then
cd $PERM_DIR
./update
elif [ $INPUT == uninstall ]
then
cd $PERM_DIR
./uninstall
elif [ $INPUT == help ]
then
cd "$PERM_DIR"
cat README.md
cd "$CURRENT_DIR"
else
echo "Not an Option"
fi
