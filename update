#!/usr/bin/env bash

clear
cd ~/Programs/autoScript
rm -R autoScript
git clone https://github.com/Treebug842/autoScript.git
cd ~/Programs/autoScript/autoScript
mv autoScript.sh ~/Programs/autoScript
mv update ~/Programs/autoScript
cd ~/Programs/autoScript
rm -R autoScript
chmod +x autoScript.sh
chmod +x update
cd ~
clear
